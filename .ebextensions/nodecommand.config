    NodeCommand: "npm run start:prod"
  aws:elasticbeanstalk:environment:proxy:staticfiles:
    /static: /server/public
  aws:elasticbeanstalk:application:environment:
    NODE_ENV: production

container_commands:
  01_install_dependencies:
    command: "npm install"
  02_build_app:
    command: "npm run build"